<h1>Artist: <%= @artist.name %></h1>
<h3>Biograph:</h3>
<p><%= @artist.bio %></p>
<h3>Songs:</h3>
<ul>
    <% @artist.songs.each do |song|%>
        <li><%= song.name %></li>
    <% end %>
</ul>
<h3>Genres:</h3>
<ul>
    <% @artist.genres.each do |genre|%>
        <li><%= genre.name %></li>
    <% end %>
</ul>